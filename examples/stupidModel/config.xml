<?xml version="1.0" ?>
<config>  
	<!-- Global simulation parameters -->
	<output resultsFile="./data/stupidModel.h5" logsDir="./logs"/>
	<seed value="1234"/>
	<logs printInConsole="false" printInstrumentation="true"/>
	<mpi overlapSizeMPI="2" subpartitioningMode="4" initialPartitioning="true" rebalancingFreq="3" maximumPercOfUnbalance="50.0" loadThreshold="0.05"/>

	<numSteps value="5" serializeResolution="1"/>
	<size width="1000" height="1000"/>
	<Bugs numBugs="10000" maxBugMovement="2" maxFoodConsumption="10" survivalProbability="95" initialBugSizeMean="1" initialBugSizeSD="0.7"/>
	<Predators numPredators="0" maxPredatorHuntDistance="1"/>
	<Raster maxFoodProduction = "0"/>
</config>
